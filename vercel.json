{
  "version": 2,
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/login",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin",
      "destination": "/api/index.py"
    },
    {
      "source": "/logout",
      "destination": "/api/index.py"
    },
    {
      "source": "/home",
      "destination": "/client/index.html"
    },
    {
      "source": "/aboutus",
      "destination": "/client/about.html"
    },
    {
      "source": "/portfolio",
      "destination": "/client/wpage.html"
    },
    {
      "source": "/contactus",
      "destination": "/client/ct.html"
    },
    {
      "source": "/favicon.jpg",
      "destination": "/client/favicon.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ]
}
