{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/home",
      "destination": "/app.py"
    },
    {
      "source": "/aboutus",
      "destination": "/app.py"
    },
    {
      "source": "/portfolio",
      "destination": "/app.py"
    },
    {
      "source": "/contactus",
      "destination": "/app.py"
    },
    {
      "source": "/login",
      "destination": "/app.py"
    },
    {
      "source": "/admin",
      "destination": "/app.py"
    },
    {
      "source": "/logout",
      "destination": "/app.py"
    },
    {
      "source": "/favicon.jpg",
      "destination": "/client/favicon.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ]
}
