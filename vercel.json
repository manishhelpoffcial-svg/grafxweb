{
  "version": 2,
  "framework": null,
  "rewrites": [
    {
      "source": "/api/inquiries",
      "destination": "/app.py"
    },
    {
      "source": "/api/inquiries/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/home",
      "destination": "/app.py"
    },
    {
      "source": "/aboutus",
      "destination": "/app.py"
    },
    {
      "source": "/portfolio",
      "destination": "/app.py"
    },
    {
      "source": "/contactus",
      "destination": "/app.py"
    },
    {
      "source": "/login",
      "destination": "/app.py"
    },
    {
      "source": "/admin",
      "destination": "/app.py"
    },
    {
      "source": "/logout",
      "destination": "/app.py"
    },
    {
      "source": "/favicon.jpg",
      "destination": "/client/favicon.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ]
}
